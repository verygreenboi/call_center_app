<ul class="list-group">
	<% @u.each do |user|%>
		<li class="list-group-item">
		<% unless user.name.nil? %>
			<%= link_to user.name, user_path(user.id) %>
		<% else %>
			<%= link_to user.email, user_path(user.id) %>
		<%end%>
		| <%= link_to "Edit", edit_user_path(user.id)%> 
		| <%= link_to "Remove", user_path(user.id), :method=>'delete', data: { confirm: "You sure?" }%></li>
	<% end %>
</ul>